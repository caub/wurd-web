{
  "name": "wurd-web",
  "version": "7.1.0",
  "description": "Vanilla JS browser client for the Wurd CMS",
  "main": "dist/wurd.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "webpack --progress --colors --watch",
    "test": "mocha --require @babel/register --colors ./src/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./src/*.spec.js",
    "example": "serve examples/simple"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "get-property-value": "^2.0.0",
    "marked": "^0.8.0",
    "mocha": "^7.1.0",
    "serve": "^11.3.0",
    "sinon": "^9.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "keywords": [
    "cms",
    "headless",
    "content",
    "browser"
  ],
  "author": "Charles Davison",
  "license": "MIT",
  "dependencies": {},
  "files": [
    "*.md",
    "dist",
    "src",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wurdcms/wurd-web.git"
  },
  "homepage": "https://github.com/wurdcms/wurd-web"
}
