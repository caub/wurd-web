{
  "name": "wurd-web",
  "version": "3.0.0",
  "description": "Browser JS client for the Wurd CMS",
  "main": "dist/wurd.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "examples": "serve examples"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "serve": "^5.1.5",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "keywords": [
    "cms",
    "headless",
    "content",
    "browser"
  ],
  "author": "Charles Davison",
  "license": "MIT",
  "dependencies": {
    "get-property-value": "2.0.0"
  }
}
