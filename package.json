{
  "name": "wurd-web",
  "version": "6.0.0",
  "description": "Browser JS client for the Wurd CMS",
  "main": "dist/wurd.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./src/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./src/*.spec.js",
    "example": "serve examples/simple"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "get-property-value": "^2.0.0",
    "marked": "^0.4.0",
    "mocha": "5.2.x",
    "serve": "9.4.x",
    "sinon": "6.1.x",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "keywords": [
    "cms",
    "headless",
    "content",
    "browser"
  ],
  "author": "Charles Davison",
  "license": "MIT",
  "dependencies": {},
  "files": [
    "*.md",
    "dist",
    "src",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wurdcms/wurd-web.git"
  },
  "homepage": "https://github.com/wurdcms/wurd-web"
}
